{
    "env": {
        "build": {
            "presets": [
              "react",
              "env",
              "stage-0"
            ],
            "plugins": [
              "add-module-exports",
              "system-import-transformer"
            ]
        },
        "es": {
            "presets": [
              ["env", {"loose": true, "modules": false}],
              "react",
              "stage-0"
            ],
            "plugins": [
              "transform-react-constant-elements"
            ]
        }
    }
}
